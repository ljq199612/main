    <select id="ScheduleEmployeeNum" parameterType="java.lang.Integer" resultType="java.lang.Integer">
        SELECT
        COUNT(*) AS schduled_num
        FROM
        bpo_classes_user_rel A
        WHERE
        A.class_generate_id = #{classGenerateId}
        AND A.`status` = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
    </select>

    <select id="findAll" resultMap="MinaScheduleToEmployeeShowMap">
        SELECT
        A.*,
        B.project_name AS project_name,
        C.work_group_id AS work_group_id,
        D.group_name AS workgroup_name,
        E.employee_num
        FROM
        bpo_classes_generate A
        JOIN bpo_project B
        ON A.project_id = B.id
        AND A.`status` = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
        LEFT JOIN bpo_classes_group_rel C ON C.class_generate_id = A.id
        LEFT JOIN bpo_work_group D ON D.id = C.work_group_id
        LEFT JOIN (
        SELECT
        class_generate_id,
        COUNT(*) AS employee_num
        FROM
        bpo_classes_user_rel
        WHERE
        `status` = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
        GROUP BY
        class_generate_id
        ) E ON E.class_generate_id = A.id
    </select>


    <select id="selectByProjectIds" resultMap="MinaScheduleToEmployeeShowMap">
        SELECT
        A.*,
        B.project_name AS project_name,
        C.work_group_id AS work_group_id,
        D.group_name AS workgroup_name,
        E.employee_num
        FROM
        bpo_classes_generate A
        JOIN bpo_project B
        ON A.project_id = B.id
        AND A.`status` = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
        AND A.project_id
        IN
        <foreach collection="list" item="pId" index="index" open="(" close=")" separator=",">
            #{pId}
        </foreach>
        LEFT JOIN bpo_classes_group_rel C ON C.class_generate_id = A.id
        LEFT JOIN bpo_work_group D ON D.id = C.work_group_id
        LEFT JOIN (
        SELECT
        class_generate_id,
        COUNT(*) AS employee_num
        FROM
        bpo_classes_user_rel
        WHERE
        `status` = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
        GROUP BY
        class_generate_id
        ) E ON E.class_generate_id = A.id
    </select>



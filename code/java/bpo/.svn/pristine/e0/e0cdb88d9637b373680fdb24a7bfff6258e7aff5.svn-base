import request from '@/utils/request'

// 类型管理
export function insertDt(query) {
  return request({
    url: '/sysDictType/insert',
    method: 'get',
    params: query
  })
}
export function findAllDt(query) {
  return request({
    url: '/sysDictType/findAll',
    method: 'get',
    params: query
  })
}
export function updateDt(query) {
  return request({
    url: '/sysDictType/update',
    method: 'get',
    params: query
  })
}

export function deleteDt(query) {
  return request({
    url: '/sysDictType/delete',
    method: 'get',
    params: query
  })
}


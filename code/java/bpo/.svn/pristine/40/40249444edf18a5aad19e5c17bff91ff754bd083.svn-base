import request from '@/utils/request'

// 获取全部的省
export function getAllProvince(query) {
  return request({
    url: '/province/getAllProvince',
    method: 'get',
    params: query
  })
}
// 根据省code获取市
export function getCity(query) {
  return request({
    url: '/province/getCity',
    method: 'get',
    params: query
  })
}

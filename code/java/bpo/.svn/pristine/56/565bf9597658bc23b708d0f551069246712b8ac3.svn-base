import request from '@/utils/request'

export function findAllModule(query) {
  return request({
    url: '/sysWorkModuleRel/findAll',
    method: 'get',
    params: query
  })
}
export function insertModule(query) {
  return request({
    url: '/sysWorkModuleRel/insert',
    method: 'get',
    params: query
  })
}
export function updateModule(query) {
  return request({
    url: '/sysWorkModuleRel/update',
    method: 'get',
    params: query
  })
}
export function insertProOrGrp(query) {

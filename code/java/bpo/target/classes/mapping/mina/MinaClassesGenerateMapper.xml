        <foreach collection="list" item="classId" index="index" open="(" close=")" separator=",">
            #{classId}
        </foreach>

    </select>


    <select id="selectByProjectIds" resultMap="MinaClassesGeneratePlusMap">
        select
            bpo_classes_generate.* ,
            bpo_project.project_name as project_name
        from
            bpo_classes_generate
        join bpo_project
        on
            bpo_classes_generate.project_id = bpo_project.id
        and bpo_classes_generate.status = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
        and bpo_classes_generate.project_id
        in
            <foreach collection="list" item="pId" index="index" open="(" close=")" separator=",">

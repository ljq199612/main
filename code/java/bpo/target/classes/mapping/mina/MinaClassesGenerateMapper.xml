    <select id="selectByPrimaryKey" resultMap="MinaClassesGeneratePlusMap" parameterType="java.lang.Integer">
        SELECT
            bpo_classes_generate.*, bpo_project.project_name AS project_name
        FROM
            bpo_classes_generate
        JOIN
            bpo_project
        ON bpo_classes_generate.project_id = bpo_project.id
        AND bpo_classes_generate.id = #{id}
    </select>

    <select id="findAll" resultMap="MinaClassesGeneratePlusMap">
          SELECT
              bpo_classes_generate.*, bpo_project.project_name AS project_name
          FROM
              bpo_classes_generate
          JOIN
              bpo_project ON bpo_classes_generate.project_id = bpo_project.id
          WHERE
              bpo_classes_generate.status = ${@com.rz.iot.bpo.common.constant.DictDataConstants@NORMAL_STATUS}
    </select>

